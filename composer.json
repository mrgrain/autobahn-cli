{
  "name": "mrgrain/autobahn-cli",
  "description": "Command line interface for Autobahn. Supports you with installing and configuring your WordPress.",
  "type": "project",
  "extra": {
    "branch-alias": {
      "dev-master": "0.2.x-dev"
    }
  },
  "require": {
    "php": ">=5.6",
    "symfony/console": "2.7.* | ^3.0",
    "paragonie/random_compat": "^2.0",
    "vlucas/phpdotenv": "^2.2",
    "wp-cli/wp-cli": "^0.23 || ^1.0.0",
    "symfony/process": "^2.8",
    "tivie/php-os-detector": "^1.0"
  },
  "license": "GPL-3.0+",
  "authors": [
    {
      "name": "Moritz Kornher",
      "email": "mail@moritzkornher.de"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Autobahn\\Cli\\": "src"
    }
  },
  "bin": [
    "bin/autobahn"
  ]
}
